{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "L4 Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./build/RC_CAR.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32L432KC",
            "configFiles": [
                "interface/stlink-v2.cfg",
                "target/stm32l4x.cfg"
            ],
            "rtos": "FreeRTOS",
            "preLaunchTask": "build and flash"
        },
        {
            "name": "L4 Debug (stop at main)",
            "cwd": "${workspaceFolder}",
            "executable": "./build/RC_CAR.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "STM32L432KC",
            "configFiles": [
                "interface/stlink-v2.cfg",
                "target/stm32l4x.cfg"
            ],
            "rtos": "FreeRTOS",
            "preLaunchTask": "build and flash"
        }
    ]
}